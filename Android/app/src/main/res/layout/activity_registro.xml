<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Registro">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteY="58dp" tools:layout_editor_absoluteX="133dp" android:id="@+id/editText"
            android:layout_alignParentEnd="true" android:layout_alignParentTop="true"
            android:layout_marginEnd="25dp" android:layout_marginTop="35dp"/>
    <TextView
            android:text="Nombre"
            android:layout_width="wrap_content"
            android:layout_height="30dp" tools:layout_editor_absoluteY="78dp"
            tools:layout_editor_absoluteX="42dp" android:id="@+id/textView2" android:layout_alignParentStart="true"
            android:layout_alignBottom="@+id/editText" android:layout_marginStart="32dp"
            android:layout_marginBottom="8dp" android:textSize="18sp"/>
    <TextView
            android:text="@string/contra"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_alignParentTop="true"
            android:layout_alignEnd="@+id/textView2" android:id="@+id/textView3" android:layout_marginTop="108dp"
            android:textSize="18sp" android:layout_marginEnd="-27dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:layout_alignBottom="@+id/textView3" android:id="@+id/editText5"
            android:layout_alignStart="@+id/editText"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:layout_alignParentTop="true" android:id="@+id/editText6" android:layout_marginTop="155dp"
            android:layout_alignStart="@+id/editText"/>
    <TextView
            android:text="@string/dob"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_alignBottom="@+id/editText6" android:id="@+id/textView4"
            android:layout_alignStart="@+id/textView2" android:textSize="18sp" android:layout_marginStart="-1dp"
            android:layout_marginBottom="22dp"/>
    <TextView
            android:text="@string/dob2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_alignBottom="@+id/editText6" android:id="@+id/textView6"
            android:layout_alignStart="@+id/textView2" android:textSize="18sp"/>
    <Button
            android:text="@string/regis"
            android:layout_width="120dp"
            android:layout_height="wrap_content" android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true" android:id="@+id/button" android:layout_marginBottom="187dp"
            android:textColor="@android:color/background_light" android:background="@android:color/holo_blue_light"/>
    <ImageView
            android:layout_width="59dp"
            android:layout_height="50dp" app:srcCompat="@drawable/logo" android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true" android:id="@+id/imageView" android:layout_marginBottom="90dp"/>
</android.widget.RelativeLayout>